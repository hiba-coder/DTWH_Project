                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select C.CustomerDwKey ,SSOD.ProductID AS ProductKey, 20050101 AS OrderDateKey,
