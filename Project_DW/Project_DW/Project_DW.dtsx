        <SQLTask:SqlTaskData
          SQLTask:Connection="{83864B08-92AE-4269-A62E-CE331F710E6C}"
          SQLTask:SqlStatementSource="INSERT INTO LIGHTADVENTUREWORKSDW2.dbo.Customers&#xA;(CustomerDwKey,  CustomerKey,  FullName,  EmailAddress,  Birthdate, MaritalStatus, &#xA;Gender, Education, Occupation, City, StateProvince, CountryRegion)&#xA;SELECT NEXT VALUE FOR LIGHTADVENTUREWORKSDW2.dbo.SeqCustomerDwKey AS CustomerDwKey,&#xA;PP.BusinessEntityID AS CustomerKey, PP.FirstName  +  '  '  +  PP.LastName  AS  FullName,PE.EmailAddress,&#xA;HE.BirthDate , HE.MaritalStatus ,HE.Gender,NULL AS Education,NULL AS Occupation,&#xA;PA.City,PS.NAME AS StateProvince, PC.NAME AS CountryRegion&#xA;&#xA;FROM AdventureWorks2012.Person.Person AS PP ,&#xA;AdventureWorks2012.Person.EmailAddress AS PE,&#xA;AdventureWorks2012.Person.BusinessEntityAddress AS PBEA,&#xA;AdventureWorks2012.HumanResources.Employee AS HE,&#xA;AdventureWorks2012.Person.Address AS PA ,&#xA;AdventureWorks2012.Person.StateProvince AS PS,&#xA;AdventureWorks2012.Person.CountryRegion AS PC&#xA;&#xA;where &#xA;PP.BusinessEntityID = PE.BusinessEntityID AND&#xA;PP.BusinessEntityID = HE.BusinessEntityID AND&#xA;PP.BusinessEntityID = PBEA.BusinessEntityID AND&#xA;PA.AddressID = PBEA.AddressID AND&#xA;PA.StateProvinceID = PS.StateProvinceID AND &#xA;PS.CountryRegionCode = PC.CountryRegionCode" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
